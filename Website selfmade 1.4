            }
        },0,5);

       };
       function einfahren() {
           console.log("supi")
        let x = document.getElementById("Einblendung");
        let n = 0;
        let time = setInterval(function () {
            if (n <= -300) {
                clearInterval(time)
            } else {
                console.log(n)
                x.style.left = n + "px";
                n -= 5
            }
        },0,5);

       };
      
